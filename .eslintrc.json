{
    "plugins": ["prettier"],
    "extends": ["eslint:recommended", "prettier"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },

    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules": {
        "prettier/prettier": 2,
        "prefer-const": "error"
    },
    "eslint.enabled": true,
    "prettier.eslintIntegration": true
}
